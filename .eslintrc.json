{
    "env": {
        "browser": true,
        "es2021": true
    },
    "ignorePatterns": ["dist/"] ,
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": [
            "error",
            "always"
        ],
        "quotes": [
            "error",
            "double"
        ]
        
          
    }
}